<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>throttle.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>throttle</object-name>
    <object-name>ecrou-th</object-name>
    
    <!-- Objets transparents -->
  </animation>

  <!-- Manette des gazs -->
  <animation>
    <type>translate</type>
    <object-name>throttle</object-name>
    <property>controls/engines/engine[0]/throttle</property>
    <interpolation>
      <entry><ind> 0 </ind><dep> 0.1 </dep></entry>
      <entry><ind> 1 </ind><dep> 0.0 </dep></entry>
    </interpolation>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>

</PropertyList>
