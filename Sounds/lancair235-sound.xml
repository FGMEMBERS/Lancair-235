<PropertyList>

  <fx>
    <crank>
      <name>engstart</name>
      <path>Aircraft/Lancair-235/Sounds/eng_start_ext1_part1.wav</path>
      <condition>
        <and>
          <property>/engines/engine[0]/cranking</property>
          <property>/sim/current-view/internal</property>
        </and>
      </condition>
      <volume>
        <offset>0.4</offset>
      </volume>
      <position>
        <x> -2.627 </x>
        <y>  1.740 </y>
        <z>  0.021 </z>
      </position>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </crank>

    <crank>
      <name>engstartext</name>
      <path>Aircraft/Lancair-235/Sounds/eng_start_int1_part1.wav</path>
      <condition>
        <and>
          <property>/engines/engine[0]/cranking</property>
        <not>
          <property>/sim/current-view/internal</property>
        </not>
        </and>
      </condition>
      <volume>
        <offset>0.4</offset>
      </volume>
      <position>
        <x> -2.627 </x>
        <y>  1.740 </y>
        <z>  0.021 </z>
      </position>
      <reference-dist>100</reference-dist>
      <max-dist>1000</max-dist>
    </crank>

		<crank>
			<name>crank</name>
			<mode>looped</mode>
			<path>Aircraft/Lancair-235/Sounds/eng_start_int1_part2.wav</path>
			<condition>
				<and>
					<property>/engines/engine[0]/cranking</property>
					<property>/sim/current-view/internal</property>
					<not>
						<property>/engines/engine[0]/running</property>
					</not>
				</and>
			</condition>
			<volume>
				<offset>0.5</offset>
			</volume>
			<reference-dist>100</reference-dist>
			<max-dist>1000</max-dist>
			<position>
				<x>-2.627</x>
				<y>1.740</y>
				<z>0.021</z>
			</position>
		</crank>

		<crank>
			<name>crankext</name>
			<mode>looped</mode>
			<path>Aircraft/Lancair-235/Sounds/eng_start_ext1_part2.wav</path>
			<condition>
				<and>
					<property>/engines/engine[0]/cranking</property>
					<not>
						<property>/sim/current-view/internal</property>
					</not>
					<not>
						<property>/engines/engine[0]/running</property>
					</not>
				</and>
			</condition>
			<volume>
				<offset>0.5</offset>
			</volume>
			<reference-dist>100</reference-dist>
			<max-dist>1000</max-dist>
			<position>
				<x>-2.627</x>
				<y>1.740</y>
				<z>0.021</z>
			</position>
		</crank>

		<crank>
			<name>cough</name>
			<path>Sounds/coughing.wav</path>
			<property>/engines/engine[0]/running</property>
			<volume>
				<offset>0.3</offset>
			</volume>
			<reference-dist>100</reference-dist>
			<max-dist>1000</max-dist>
			<position>
				<x>-2.627</x>
				<y>1.740</y>
				<z>0.021</z>
			</position>
		</crank>

		<stopext>
			<name>stopext</name>
			<path>Aircraft/Lancair-235/Sounds/eng_stop_ext1.wav</path>
			<condition>
				<not>
					<property>/engines/engine[0]/running</property>
				</not>
			</condition>
			<volume>
				<property>/sim/current-view/internal</property>
				<factor>-1.0</factor>
				<offset>1.0</offset>
			</volume>
			<reference-dist>100</reference-dist>
			<max-dist>1000</max-dist>
			<position>
				<x>-2.627</x>
				<y>1.740</y>
				<z>0.021</z>
			</position>
		</stopext>

		<stop>
			<name>stop</name>
			<path>Aircraft/Lancair-235/Sounds/eng_stop_int1.wav</path>
			<condition>
				<not>
					<property>/engines/engine[0]/running</property>
				</not>
			</condition>
			<volume>
				<property>/sim/current-view/internal</property>
				<factor>1.0</factor>
				<offset>0.0</offset>
			</volume>
			<reference-dist>100</reference-dist>
			<max-dist>1000</max-dist>
			<position>
				<x>-2.627</x>
				<y>1.740</y>
				<z>0.021</z>
			</position>
		</stop>

		<engine>
			<name>engine</name>
			<mode>looped</mode>
			<path>Aircraft/Lancair-235/Sounds/eng_run_ext1_loop.wav</path>
			<condition>
				<and>
					<property>/engines/engine[0]/running</property>
					<not>
						<property>/sim/current-view/internal</property>
					</not>
				</and>
			</condition>
			<volume>
				<property>engines/engine[0]/mp-osi</property>
				<factor>0.00375</factor>
				<min>0.5</min>
				<max>2.0</max>
			</volume>
			<pitch>
				<property>/engines/engine[0]/rpm</property>
				<factor>0.00028</factor>
				<min>0.15</min>
				<max>2.0</max>
				<offset>-0.0</offset>
			</pitch>
			<reference-dist>100</reference-dist>
			<max-dist>1000</max-dist>
			<position>
				<x>-2.627</x>
				<y>1.740</y>
				<z>0.021</z>
			</position>
		</engine>

		<engine>
			<name>engineextslow</name>
			<mode>looped</mode>
			<path>Aircraft/Lancair-235/Sounds/eng_run_ext3_slow.wav</path>
			<condition>
				<and>
					<property>/engines/engine[0]/running</property>
					<not>
						<property>/sim/current-view/internal</property>
					</not>
				</and>
			</condition>
			<volume>
				<property>engines/engine[0]/mp-osi</property>
				<factor>0.00375</factor>
				<min>0.5</min>
				<max>2.0</max>
			</volume>
			<pitch>
				<property>/engines/engine[0]/rpm</property>
				<factor>0.0005</factor>
				<min>0.35</min>
				<max>2.0</max>
				<offset>-0.0</offset>
			</pitch>
			<reference-dist>100</reference-dist>
			<max-dist>1000</max-dist>
			<position>
				<x>-2.627</x>
				<y>1.740</y>
				<z>0.021</z>
			</position>
		</engine>

		<propellerhigh>
			<name>propellerhigh</name>
			<mode>looped</mode>
			<path>Aircraft/Lancair-235/Sounds/eng_run_int_4430.wav</path>
			<condition>
				<and>
					<property>/engines/engine[0]/running</property>
					<property>/sim/current-view/internal</property>
				</and>
			</condition>
			<volume>
				<property>engines/engine[0]/mp-osi</property>
				<factor>0.00375</factor>
				<min>0.5</min>
				<max>2.0</max>
			</volume>
			<!--
			<volume>
			<property>/orientation/alpha-deg</property>
			<type>log</type>
			<min>0.2</min>
			<max>1.0</max>
			</volume>
			<volume>
			<property>/sim/current-view/internal</property>
			<factor>0.5</factor>
			</volume>-->
			<pitch>
				<property>/engines/engine[0]/rpm</property>
				<factor>0.000226</factor>
				<min>0.15</min>
				<max>4.0</max>
				<offset>-0.0</offset>
			</pitch>
			<reference-dist>100</reference-dist>
			<max-dist>1000</max-dist>
			<position>
				<x>-2.627</x>
				<y>1.740</y>
				<z>0.021</z>
			</position>
		</propellerhigh>

		<propellerlow>
			<name>propellerlow</name>
			<mode>looped</mode>
			<path>Aircraft/Lancair-235/Sounds/eng_run_int_1340.wav</path>
			<condition>
				<and>
					<property>/engines/engine[0]/running</property>
					<property>/sim/current-view/internal</property>
				</and>
			</condition>
			<volume>
				<property>engines/engine[0]/mp-osi</property>
				<factor>0.00375</factor>
				<min>0.5</min>
				<max>2.0</max>
			</volume>
			<!--
			<volume>
			<property>/orientation/alpha-deg</property>
			<type>log</type>
			<min>0.2</min>
			<max>1.0</max>
			</volume>
			<volume>
			<property>/sim/current-view/internal</property>
			<factor>0.5</factor>
			</volume>
			-->
			<pitch>
				<property>/engines/engine[0]/rpm</property>
				<factor>0.0007462</factor>
				<min>0.5</min>
				<max>4.0</max>
				<offset>-0.0</offset>
			</pitch>
			<reference-dist>100</reference-dist>
			<max-dist>1000</max-dist>
			<position>
				<x>-2.627</x>
				<y>1.740</y>
				<z>0.021</z>
			</position>
		</propellerlow>

		<rumble>
			<name>rumble</name>
			<mode>looped</mode>
			<path>Aircraft/Lancair-235/Sounds/rumble.wav</path>
			<condition>
				<or>
					<property>/gear/gear[0]/wow</property>
					<property>/gear/gear[1]/wow</property>
					<property>/gear/gear[2]/wow</property>
				</or>
				<!-- <property>/sim/current-view/internal</property> -->
			</condition>
			<volume>
				<property>/velocities/groundspeed-kt</property>
				<!--type>log</type-->
				<factor>0.025</factor>
				<offset> -0.01</offset>
				<max>1.01</max>
				<min>0.01</min>
			</volume>
			<pitch>
				<property>/velocities/groundspeed-kt</property>
				<factor>0.01</factor>
				<offset>0.85</offset>
			</pitch>
			<reference-dist>100</reference-dist>
			<max-dist>1000</max-dist>
		</rumble>

		<squeal>
			<name>squeal</name>
			<path>Aircraft/Lancair-235/Sounds/wow.wav</path>
			<condition>
				<or>
					<property>/gear/gear[0]/wow</property>
					<property>/gear/gear[1]/wow</property>
					<property>/gear/gear[2]/wow</property>
				</or>
			</condition>
			<volume>
				<property>/velocities/speed-down-fps</property>
				<factor>0.5</factor>
				<max>1.0</max>
			</volume>
			<volume>
				<property>/velocities/airspeed-kt</property>
				<factor>0.1</factor>
				<max>1.0</max>
			</volume>
			<volume>
				<internal>dt_stop</internal>
				<factor>0.05</factor>
				<max>1.0</max>
			</volume>
			<pitch>
				<property>/velocities/airspeed-kt</property>
				<factor>0.0025</factor>
				<offset>1.0</offset>
			</pitch>
			<reference-dist>100</reference-dist>
			<max-dist>1000</max-dist>
		</squeal>

